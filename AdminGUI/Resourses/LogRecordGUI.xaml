<UserControl x:Class="AdminGUI.Resourses.LogRecordGUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:src="clr-namespace:AdminGUI.Resourses"
             mc:Ignorable="d" 
             >
    <UserControl.Resources>
        <src:StringToDataConverter x:Key="StringToDataConverter" />
    </UserControl.Resources>
    <StackPanel Orientation="Horizontal">
        <Label Content="{Binding Path=Time}"  />
        <Label Content="{Binding Path=User}" />
        <Label Content="{Binding Path=MValue}" />
        <Label Content="{Binding Path=SEvent}" />
        <Label Content="{Binding Path=SValue}" />
    </StackPanel>
</UserControl>
